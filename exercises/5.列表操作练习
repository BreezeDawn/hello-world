vlist = [0,1,2,3,5,4]

#列表*2
vlist*2
- [0, 1, 2, 3, 5, 4, 0, 1, 2, 3, 5, 4]

#查看列表长度
len(vlist)
- 6

#遍历列表
for i in vlist:
    print(i)
- 0
  1
  2
  3
  5
  4
 
#判断某元素是否在列表中
print(2 in vlist)
- True

#返回元素在列表中的数量
print(vlist.count(2))
- 1

#在某位置插入某元素
vlist.insert(2,22)
- [0, 1, 22, 2, 3, 5, 4]

#对列表进行排序
vlist.sort()
- [0, 1, 2, 3, 4, 5, 22]

#在列表末尾添加元素
vlist.append(222)
- [0, 1, 2, 3, 4, 5, 22, 222]

#反转列表
vlist.reverse()
- [222, 22, 5, 4, 3, 2, 1, 0]

#移除元素
vlist.remove(2)
- [222, 22, 5, 4, 3, 1, 0]

#返回某元素在列表中的索引值
vlist.index(222)
- 0

#移除某位置的元素
vlist.pop(2)
- 5
vlist
- [222, 22, 4, 3, 1, 0]
